<h1>
  Success stories en relations maths-industrie
</h1>


<table id="table" class="table table-striped table-responsive">
 <thead>
    <th>Sheet</th>
    <th>Name</th>
    <th>Short description</th>
    <th>H2020</th>
    <th>Sector</th>
    <th>Academic Partner</th>
    <th>Industrial Partner</th>
  </thead>
  <tbody>
      <% data.stories_table.each do |e|%>
	  <tr>
	      <%= link_to_sheet e.metadata_identifier %>
	      <%= surround_td(e.setup_name) %>
	      <%= surround_td(e.setup_oneline) %>
	      <%= surround_td(e.setup_h2020) %>
	      <%= surround_td(e.setup_sector) %>
	      <%= surround_td(e.setupacademic_name) %>
	      <%= surround_td(e.setupindustrial_name) %>
	  </tr>
      <% end %>
  </tbody>
</table>

<script>
$(document).ready(function(){
    $('#table').dataTable( {
	"columns": [
	{ "orderable": false , "searchable":false },
	null,
	null,
	null,
	null,
	null,
	null
	]
    } );});
</script>




